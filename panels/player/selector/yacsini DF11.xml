<panel height="270">
	<layout class="stick_to_sides_attachment" alignment="all" apply_to_children="true"/>
	<container background_colour="secondary">
		<layout class="stick_to_sides_attachment" alignment="all" apply_to_children="true"/>
		<container>
			<attachment_group class="vertical_arrange" vertical_layout="220,-1" horizontal_offset="0" vertical_offset="10" />
			<widget class="picture" scale_picture="true" keep_aspect_ratio="true" image_alignment="top">
				<record id="object_property">
					<list id="get_properties">
						<record get_property="fcgn" />
						<record get_property="Pfke" />
						<record get_property="prpc" set_property="valu" />
					</list>
				</record>
			</widget>
			<container/>
		</container>
		<container>
			<attachment_group class="vertical_arrange" vertical_layout="235,-1" horizontal_offset="0" vertical_offset="0" />
			<container/>
			<container>
				<attachment_group class="horizontal_arrange" horizontal_layout="160,-1" horizontal_gap="0"/>
				<container/>
				<container>
					<layout class="stick_to_sides_attachment" alignment="all" apply_to_children="true"/>
					<widget class="object_portrait_picture" id="pnat" scale_picture="true" keep_aspect_ratio="true">
						<layout class="stick_to_sides_attachment" alignment="right" inset="0"/>
						<record id="object_property">
							<integer id="get_property" value="PNat" />
						</record>
					</widget>
				</container>
			</container>
		</container>
		<widget class="picture" file="yacs/yacsini/2019/DF11/yacsini_main" scale_picture="true" keep_aspect_ratio="false" image_alignment="centre" red_replacement="primary" blue_replacement="secondary"/>
	</container>
	<container>
		<attachment_group class="vertical_arrange" vertical_layout="45,172,-1" horizontal_offset="0" vertical_offset="0" />
		<container>
			<attachment_group class="horizontal_arrange" horizontal_layout="40,-1" horizontal_gap="0"/>
			<widget class="squad_number_label" spec="shadowed" font="news" size="17" alternate_format="false" alignment="centre" wants_mouse_events="false" colour="secondary">
				<record id="object_property">
					<list id="get_properties">
						<record get_property="Psnu" set_property="valu" />
					</list>
				</record>
			</widget>
			<container/>
		</container>
		<container/>
		<container>
			<attachment_group class="horizontal_arrange" horizontal_layout="3,150,-1" horizontal_gap="0"/>
			<container/>
			<container>
				<attachment_group class="vertical_arrange" vertical_layout="-2,-1,12" horizontal_offset="0" vertical_offset="0" vertical_gap="-4" />
				<widget class="person_button" dslk="true" format="[%person#1-surname]" size="15" alignment="bottom,can_scale" font="signature" style="shadowed" enforce_style="true" colour="secondary">
					<record id="object_property">
						<list id="get_properties">
							<record>
								<integer id="get_property" value="objt"/>
								<integer id="set_property" value="valu"/>
							</record>
						</list>
					</record>
				</widget>
				<container>
					<attachment_group class="horizontal_arrange" horizontal_layout="-1,20" horizontal_gap="0"/>
					<widget class="international_appearances_label" id="inta" int_apps_label_format="10" size="9" colour="secondary" font="tcsfont" style="shadowed" alignment="top,can_scale">
						<record id="object_property" dont_set_hint="true" get_property="Pist" />
					</widget>
					<container/>
				</container>
				<container/>
			</container>
			<container/>
		</container>
	</container>
	<widget class="picture" file="yacs/yacsini/2019/DF11/overlay"  scale_picture="true" keep_aspect_ratio="false" transparency="0.5"/>
</panel>