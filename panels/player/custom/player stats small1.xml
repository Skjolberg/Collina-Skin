<?xml version='1.0' encoding='utf-8'?>
<panel>
  <!-- hide empty rows -->
  <boolean id="hier" value="true"/>

  <layout class="stick_to_sides_attachment" alignment="all" inset="0" apply_to_children="true" />

  <container class="icon_box" file="player/player status icon box" icon="icons/custom/icon box/stats">
    <layout class="stick_to_sides_attachment" alignment="horizontal" inset="0" apply_to_children="true" />
    <layout class="stick_to_sides_attachment" alignment="vertical" inset="-5" apply_to_children="true" />

    <!-- Set icon to only show: Yel, Sus, Una -->
    <record id="widget_properties" type="5" />

   <container id="pslt" class="inner_box" save_session_state="true" sdst="true">
		<layout class="fit_children_attachment" alignment="vertical" offset="0" gap="0" />
		<layout class="stick_to_sides_attachment" alignment="horizontal,top" inset="0" apply_to_children="true" />

		<widget class="table" id="pstt" layout="-1, 70, 50, 50, 50, 50, 50, 50, 50, 50, 50, 50, 50, 50, 50, 50" mode="fill_rows, stripe_rows,printable" row_height="22" row_spacing="0" auto_size="vertical">
			<list id="headings">
				<record alignment="left,centre_y">
					<translation id="text" translation_id="229984" type="use" value="Statistics" />
				</record>
			</list>

			<record id="default_properties" alignment="centre" />

			<list id="column_widget_properties">
				<record indx="0">
					<flags id="algn" value="left,centre_y" />
				</record>
			</list>

			<list id="column_table_properties">
				<record indx="0" sort_disabled="true" />
			</list>
		</widget>
	</container>

    <container>
      <layout class="fit_children_attachment" alignment="all" inset="4" />
      <layout class="stick_to_sides_attachment" alignment="left" apply_to_children="true" inset="0" />
      <layout class="stick_to_sides_attachment" alignment="top" apply_to_children="true" inset="4" />

      <string id="skin_style" value="title link" />

      <!-- Linked header -->
  
    </container>
  </container>
</panel>