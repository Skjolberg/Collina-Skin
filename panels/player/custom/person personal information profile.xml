			<layout class="fit_children_attachment" alignment="vertical" offset="0" gap="0" />
			<layout class="arrange_horizontal_attachment" layout="-3,-4" alignment="left" offset="2" gap="8" />
			<layout class="stick_to_sides_attachment" alignment="top" apply_to_children="true" inset="0" />
			<container class="plain_box" >
				<layout class="fit_children_attachment" alignment="vertical" offset="0" gap="0" />
				<layout class="arrange_horizontal_attachment" alignment="fill" offset="6" gap="0" />
				<layout class="stick_to_sides_attachment" alignment="top" apply_to_children="true" inset="0" />
				<widget class="table" id="left" layout="-1" mode="fill_rows" auto_size_rows="true" fixed_size_rows="false" auto_size="vertical">
					<list id="row_properties">
						<record indx="0" row_height="22" />
						<record indx="1" row_height="20" />
