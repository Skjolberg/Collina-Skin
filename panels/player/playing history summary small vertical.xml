<?xml version="1.0" encoding="utf-8"?>
<panel>
		<attachment_group class="vertical_arrange" vertical_alignment="top,extend" />
		
  <widget class="table" id="phst" layout="40,1,60,-1,35,35" mode="fill_rows, stripe_rows" fixed_size_rows="false">
    <layout class="stick_to_sides_attachment" alignment="horizontal" inset="0"/>
    <!--boolean id="all_content_same_height" value="false"/-->
    <integer id="row_spacing" value="4"/>
    <integer id="column_spacing" value="4"/>

    <list id="headings">
      <record index="0" alignment="centre_x,centre_y"/>
    </list>
    <list id="column_table_properties">
      <record index="0" sort_disabled="false" />
      <record index="1" sort_disabled="true" />
      <record index="2" sort_disabled="true" />
      <record index="3" sort_disabled="true" />
      <record index="4" sort_disabled="false" />
      <record index="5" sort_disabled="false" />
    </list>
    <list id="column_widget_properties">
      <record index="0" alignment="centre" style="vertical" height="60"/>
      <record index="1" alignment="centre" style="vertical"/>
      <record index="2" alignment="centre" scale_picture="true" keep_aspect_ratio="true" image_alignment="centre"/>
      <record index="4" alignment="centre" spec="text" />
      <record index="5" alignment="centre" spec="text"/>
    </list>
  </widget>

</panel>
