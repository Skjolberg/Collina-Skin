<properties version="1.0">

		<!--
		 ============================================================
		 Default Settings Modified By This Skin

		 The definitions below will override the defaults contained
		 in the parent skins, or in the various global and application
		 settings files.

		 ============================================================
		 -->
		
		<!-- global settings -->

		<set_global name="ui.main_box_gap" >
			<integer value="12"/>
		</set_global>
		<set_global name="ui.main_contrast_box_gap" >
			<integer value="8"/>
		</set_global>
		<set_global name="ui.inner_box_gap" >
			<integer value="20"/>
		</set_global>
		<set_global name="ui.main_content_margin" >
			<integer value="12"/>
		</set_global>
		<set_global name="ui.button_row_gap" >
			<integer value="4"/>
		</set_global>
		<set_global name="ui.split_button_row_gap" >
			<integer value="1"/>
		</set_global>


		<!-- This allows us to set a global font adjustment value for given languages this
		     is required because we map to a different font set for russian and greek which
			 ends up with a larger vertical size then the base font which causes cut offs, so now we
			 can adjust it to fit closer to the original size -->
		<list id="language_font_scaling_list">
			<record name="russian">
				<coordinate id="value" x="0.9" y="0.9" />
			</record>
			<record name="greek">
				<coordinate id="value" x="0.9" y="0.9" />
			</record>
			<record name="default">
				<coordinate id="value" x="1.0" y="1.0" />
			</record>
		</list>

